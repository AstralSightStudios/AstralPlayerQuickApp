{
  "package": "com.astralsightstudios.astralplayer",
  "name": "AstralPlayer",
  "versionName": "1.0.1",
  "versionCode": 1,
  "minPlatformVersion": 1000,
  "icon": "/common/logo.png",
  "deviceTypeList": [
    "watch"
  ],
  "features": [
    {
      "name": "system.router"
    },
    {
      "name": "system.file"
    }
  ],
  "config": {
    "logLevel": "log",
    "designWidth": 480,
    "background": {
      "features": [
        "system.audio",
        "system.request"
      ]
    }
  },
  "router": {
    "entry": "pages/index",
    "pages": {
      "pages/index": {
        "component": "index"
      },
      "pages/login": {
        "component": "login"
      },
      "pages/home": {
        "component": "home"
      },
      "pages/settings": {
        "component": "settings"
      },
      "pages/about": {
        "component": "about"
      },
      "pages/debug": {
        "component": "debug"
      },
      "pages/error": {
        "component": "error"
      },
      "pages/success": {
        "component": "success"
      },
      "pages/decoder": {
        "component": "decoder"
      },
      "pages/decoding": {
        "component": "decoding"
      },
      "pages/download": {
        "component": "download"
      },
      "pages/vidlist": {
        "component": "vidlist"
      },
      "pages/rmvid": {
        "component": "rmvid"
      },
      "pages/play": {
        "component": "play"
      }
    }
  },
  "display": {
    "backgroundColor": "#000000"
  }
}